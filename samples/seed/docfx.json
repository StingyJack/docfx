{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "assembly/bin/**/*.dll",
            "project/**/*.csproj",
            "solution/**/*.sln",
            "csharp/**/*.cs",
            "vb/**/*.vb"
          ],
          "exclude": [
            "project/Project.Core/**"
          ],
          "src": "dotnet"
        }
      ],
      "namespaceLayout": "nested",
      "dest": "obj/api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [ "**/*.yml" ],
        "src": "obj/api",
        "dest": "api"
      },
      {
        "files": [ "articles/**/*.{md,yml}", "*.md", "toc.yml", "restapi/**" ]
      }
    ],
    "resource": [
      {
        "files": [ "articles/images/**"]
      }
    ],
    "overwrite": "specs/*.md",
    "globalMetadata": {
      "_appTitle": "docfx seed website",
      "_appName": "Seed",
      "_enableSearch": true
    },
    "fileMetadata": {
      "enablePdf": {
        "**/toc.yml": true
      }
    },
    "dest": "_site",
    "exportViewModel": true,
    "template": ["default", "modern"]
  }
}
